{
  "name": "pacor",
  "version": "4.1.0",
  "adonis-version": "4.1.0",
  "description": "The fullstack application boilerplate for Adonisjs",
  "main": "index.js",
  "scripts": {
    "0.init": "npm i && node init.js && npm run migration && npm run 0.development",
    "1.development": "concurrently -p \"[{name} {time}]\" -t \"HH:mm:ss\" --kill-others --names \"a,c,w,d\" \"npm run s1.start\" \"npm run s3.start-cors\" \"npm run w2.webpack-watch-development\" \"npm run s2.start-database-admin\" ",
    "2.production": "concurrently -p \"[{name} {time}]\" -t \"HH:mm:ss\" --kill-others --names \"a,w,d\" \"npm run s1.start\" \"npm w4.webpack-build-production\" \"npm run s2.start-database-admin\" ",
    "s1.start": "nodemon --ignore webpack-app --ignore database --ignore public --ignore website-cors --ignore test --ignore node_modules --ignore .document --ignore .private-document --ignore help --ignore storage server.js",
    "s2.start-database-admin": "node ./database/hisha/bin/www",
    "s3.start-cors": "node ./website-cors/index.js",
    "s9.start-https": "nodemon --ignore webpack-app --ignore database --ignore public --ignore website-cors --ignore test --ignore node_modules --ignore .document --ignore .private-document --ignore help --ignore storage server-https.js",
    "setup-database": "adonis migration:refresh & adonis seed",
    "adonisjs-self-test": "node ace test",
    "w1.webpack-build-development": "webpack --mode development",
    "w2.webpack-watch-development": "webpack --watch --mode development --display errors-only",
    "w3.webpack-watch-build-production": "webpack --watch --mode production --display errors-only",
    "w4.webpack-build-production": "webpack --mode production",
    "test": "adonis test"
  },
  "keywords": [
    "adonisjs",
    "adonis-app",
    "vue",
    "cors"
  ],
  "author": "",
  "license": "MIT",
  "private": false,
